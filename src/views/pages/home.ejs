                                else if (index === totalServices - 2 && totalServices % 3 === 2) {
                                cardClass = "col-xl-6 col-md-6";

                                } else if (index === totalServices - 1 && totalServices % 3 === 2) {
                                cardClass = "col-xl-6 col-md-6";
                                }
                                %>

                                <div class="<%= cardClass %>">
                                    <div class="feature-card <%= heightClass %>">
